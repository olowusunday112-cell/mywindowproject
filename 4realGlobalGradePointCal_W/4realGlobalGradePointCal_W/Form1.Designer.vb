    Private Sub Guna2Button15_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Guna2Button15.Click
        swd_txt.Clear()
        da_txt.Clear()
        nt_txt.Clear()
        wad_txt.Clear()
        mad_txt.Clear()
        uiux_txt.Clear()

        grade_txt.Clear()
        gradePoint_txt.Clear()
        totalScorePoint_txt.Clear()
        swd_txt.Focus()
        Application.Exit()


    End Sub
