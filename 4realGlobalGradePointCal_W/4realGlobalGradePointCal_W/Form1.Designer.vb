    Private Sub calculatebtn_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles calculatebtn.Click

        Dim swd As Double = Val(swd_txt.Text)
        Dim da As Double = Val(da_txt.Text)
        Dim nt As Double = Val(nt_txt.Text)
        Dim wad As Double = Val(wad_txt.Text)
        Dim mad As Double = Val(mad_txt.Text)
        Dim uiux As Double = Val(uiux_txt.Text)


        If swd > 100 Or da > 100 Or nt > 100 Or wad > 100 Or mad > 100 Or uiux > 100 Then
            MessageBox.Show("INVALID!!! SCORE MUST NOT EXCEED 100")
            Exit Sub
        End If


        Dim p1 As Double = MyFunction.gradepoint(swd, 3)
        Dim p2 As Double = MyFunction.gradepoint(da, 4)
        Dim p3 As Double = MyFunction.gradepoint(nt, 3)
        Dim p4 As Double = MyFunction.gradepoint(wad, 4)
        Dim p5 As Double = MyFunction.gradepoint(mad, 4)
        Dim p6 As Double = MyFunction.gradepoint(uiux, 2)


        Dim totalSP As Double = p1 + p2 + p3 + p4 + p5 + p6
        Dim gp As Double = totalSP / 20 ' 20 is the total units (3+4+3+4+4+2)
        Dim gradeResult As String = ""


        If (gp >= 3.5) Then
            gradeResult = "DISTINCTION"
        ElseIf (gp >= 3.0) Then
            gradeResult = "UPPER CLASS"
        ElseIf (gp >= 2.5) Then
            gradeResult = "LOWER CLASS"
        ElseIf (gp >= 2.0) Then
            gradeResult = "PASS"
        Else
            gradeResult = "FAIL"
        End If


        totalScorePoint_txt.Text = totalSP.ToString()
        gradePoint_txt.Text = Math.Round(gp, 2).ToString()
        grade_txt.Text = gradeResult


        ' Check if any text box is empty before calculating
        If swd_txt.Text = "" Or da_txt.Text = "" Or nt_txt.Text = "" Or
           wad_txt.Text = "" Or mad_txt.Text = "" Or uiux_txt.Text = "" Then

            MessageBox.Show("Please enter scores for ALL courses before calculating.",
                            "Missing Information",
                            MessageBoxButtons.OK,
                            MessageBoxIcon.Warning)
            Exit Sub ' This stops the code from running the math below
        End If
    End Sub

